{
  "name": "my-trips",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "codegen": "graphql-codegen --config codegen.yml"
